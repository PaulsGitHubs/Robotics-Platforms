# Example environment variables for local development
# Copy this to a file named `.env` at the project root (do NOT commit your real keys)

# Your Cesium Ion access token (get one at https://cesium.com/ion/settings)
CESIUM_ION_TOKEN=your_cesium_ion_token_here

# For development only: allow server to inject token into the client template
# WARNING: exposing tokens to client-side templates is a security risk; only enable locally
ALLOW_CESIUM_TOKEN_IN_TEMPLATE=true


OPENAI_API_KEY=your_open_api_key
